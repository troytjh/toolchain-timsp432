# config version=3.5
$ uscif
  ecom_drvr=xds2xxu.out
  ecom_port=0
$ /
$ xds2xx
  bio_0="BIO_0,Active_High"
  bio_1="BIO_1,Active_High"
  bio_2="BIO_2,Active_High"
  bio_3="BIO_3,Active_High"
  txd1="TXD1,Active_High"
  rxd1="RXD1,Active_High"
  expio_4="EXPIO_4,Active_High"
  expio_5="EXPIO_5,Active_High"
  expio_6="EXPIO_6,Active_High"
  expio_7="EXPIO_7,Active_High"
$ /
$ uscif
  jtagboot_mode=disable
  jtagboot_value=hiz
  powerboot_mode=disable
  powerboot_value=hiz
  tclk_program=SPECIFIC
  tclk_frequency=10.0MHz
  loopback_mode=disable
  loopback_value=disable
  jtag_isolate=enable
  extrareset_system=false
$ /
$ dot7
  dts_usage=nothing
$ /
$ swd
  swd_debug=enabled
  swo_data=tdo_pin
$ /
@ cs_dap_0 family=cs_dap irbits=4 drbits=1 subpaths=1 identify=0
  & subpath_0 type=legacy address=16 default=no custom=no force=yes pseudo=yes
    @ cortex_m4_0 family=cortex_mxx irbits=0 drbits=0 identify=0x02000000 traceid=0x0
  & /
# /
